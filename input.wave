canvas width 100 height 100 background [0, 0, 0];

gradientX = $x / 100
gradientY = $y / 100
red = 255 * $gradientX
green = 255 * $gradientY
blue = 255 - (255 * $gradientY)
clr = [$red, $green, $blue]

repeat x --> 100 {
    repeat y --> 100 {
        color $clr;
        rect from[$x, $y] to[$x, $y];
    }
}

write file "funny_rectangle.png";